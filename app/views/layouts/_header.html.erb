<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "Members Only", posts_path, id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Posts", posts_path, id: "header_link" %></li>
        <% if logged_in? %>
          <li><%= link_to "Publish", new_post_path, id: "header_link" %></li>
          <li><%= link_to "Log out", logout_path, id: "header_link", method: :delete %></li>
        <% else %>
          <li><%= link_to "Log in", login_path, id: "header_link" %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
